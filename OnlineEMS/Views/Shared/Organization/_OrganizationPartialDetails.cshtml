





        <table class="table table-striped table-bordered bootstrap-datatable datatable">
            <thead>
            <tr>
                <th>SL</th>
                <th>Organization Name</th>
                <th>Code</th>
                <th>Address</th>
                <th>Contact No</th>
                <th>About</th>
                <th>Logo Name</th>
                <th>Actions</th>
            </tr>
            </thead>
            <tbody>

                @if (Model != null && Model.Organizations != null && Model.Organizations.Count > 0)
                {
                    var id = 5;
                    foreach (var organization in Model.Organizations)
                    {
                        <tr>
                            <td>@(id--)</td>
                            <td>@organization.Name</td>
                            <td class="center">@organization.Code</td>
                            <td class="center">@organization.Address</td>
                            <td class="center">@organization.ContactNo</td>
                            <td class="center">@organization.About</td>
                            <td class="center">@organization.LogoName</td>
                            <td class="center" width="250">
                                <a class="btn btn-success" href="/Organization/Details/@organization.Id">
                                    <i class="halflings-icon white zoom-in"></i>
                                </a>
                                <a class="btn btn-info" href="/Organization/Edit/@organization.Id">
                                    <i class="halflings-icon white edit"></i>
                                </a>
                                @*<a class="btn btn-danger" href="/Organization/Delete/@organization.Id">
                                    <i class="halflings-icon white trash"></i>
                                </a>*@
                            </td>
                        </tr>
                    }

                }




            </tbody>

        </table>

   
